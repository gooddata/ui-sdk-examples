{
  "name": "gd-ui-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@gooddata/react-components": "^7.1.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-highcharts": "^16.0.2",
    "react-scripts": "2.0.5",
    "react-select": "^3.1.0"
  },
  "scripts": {
    "start": "cross-env HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
